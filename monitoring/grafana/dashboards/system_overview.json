{
  "dashboard": {
    "id": null,
    "title": "TalentAI - System Overview",
    "tags": ["talentai", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "talentai_system_health",
            "legendFormat": "{{component}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "API Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(talentai_api_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "API Response Time (99th percentile)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(talentai_api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "99th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(talentai_api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(talentai_api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (seconds)"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Resume Processing Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(talentai_resume_processing_total[5m])",
            "legendFormat": "{{parsing_method}} - {{status}}"
          }
        ],
        "yAxes": [
          {
            "label": "Processing Rate"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Search Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(talentai_search_requests_total[5m])",
            "legendFormat": "{{blind_screening}} - {{status}}"
          }
        ],
        "yAxes": [
          {
            "label": "Search Rate"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Authentication Attempts",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(talentai_auth_attempts_total[5m])",
            "legendFormat": "{{auth_type}} - {{status}}"
          }
        ],
        "yAxes": [
          {
            "label": "Auth Rate"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "Database Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(talentai_db_operations_total[5m])",
            "legendFormat": "{{collection}} - {{operation}}"
          }
        ],
        "yAxes": [
          {
            "label": "Operations/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}